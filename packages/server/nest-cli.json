{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "webpackConfigPath": "webpack.config.js"
  },
  "projects": {
    "api": {
      "type": "library",
      "root": "libs/api",
      "entryFile": "index",
      "sourceRoot": "libs/api/src",
      "compilerOptions": {
        "tsConfigPath": "libs/api/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "data": {
      "type": "library",
      "root": "libs/data",
      "entryFile": "index",
      "sourceRoot": "libs/data/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "serverless": {
      "type": "library",
      "root": "libs/serverless",
      "entryFile": "index",
      "sourceRoot": "libs/serverless/src",
      "compilerOptions": {
        "tsConfigPath": "libs/serverless/tsconfig.lib.json"
      }
    },
    "static": {
      "type": "library",
      "root": "libs/static",
      "entryFile": "index",
      "sourceRoot": "libs/static/src",
      "compilerOptions": {
        "tsConfigPath": "libs/static/tsconfig.lib.json"
      }
    },
    "zjuer": {
      "type": "library",
      "root": "libs/zjuer",
      "entryFile": "index",
      "sourceRoot": "libs/zjuer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/zjuer/tsconfig.lib.json"
      }
    }
  }
}