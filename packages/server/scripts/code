class Solution:
    def maxFrequency(self, nums: List[int], k: int, numOperations: int) -> int:
        c = Counter(nums)
        nums.sort()
        n = len(nums)
        res = 0
        for num in range(nums[0], nums[-1] + 1):
            lidx = bisect_left(nums, num - k)
            ridx = bisect_right(nums, num + k)
            cnt = min(ridx - lidx - c[num], numOperations) + c[num]
            res = max(res, cnt)
        return res