class Solution:
    def countTrapezoids(self, points: List[List[int]]) -> int:
        mod = 10 ** 9 + 7
        res = 0
        counter = Counter()
        for x, y in points:
            counter[y] += 1
        arr = list(counter.values())
        compute = lambda v: (1 + v - 1) * (v - 1) // 2
        prev = compute(arr[0])
        for curr in arr[1:]:
            v = compute(curr)
            res += prev * v
            prev += v
        return res % mod