class Solution:
    def findXSum(self, nums: List[int], k: int, x: int) -> List[int]:
        n = len(nums)
        res = []
        for i in range(n - k + 1):
            sub_nums = sorted(Counter(nums[i: i + k]).items(), key = lambda v: (v[1], v[0]), reverse = True)[:x]
            res.append(sum(num * cnt for num, cnt in sub_nums))
        return res