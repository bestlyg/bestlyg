class Solution:
    def sumFourDivisors(self, nums: List[int]) -> int:
        res = 0
        for num in nums:
            item = num + 1
            cnt = 2
            v = 2
            while v ** 2 < num:
                if num % v == 0:
                    cnt += 2
                    item += v + num // v
                v += 1
            if v ** 2 == num:
                cnt += 1
                item += v
            if cnt == 4:
                res += item
        return res