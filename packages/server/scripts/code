def count_odds(n: int):
    if n % 2 != 0: n += 1
    return n // 2
class Solution:
    def flowerGame(self, n: int, m: int) -> int:
        res = 0
        nodd = count_odds(n)
        neven = n - nodd
        modd = count_odds(m)
        meven = m - modd
        res += nodd * meven
        res += neven * modd
        return res