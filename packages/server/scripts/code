class Solution:
    def compareVersion(self, version1: str, version2: str) -> int:
        l1 = list(map(int, version1.split('.')))
        l2 = list(map(int, version2.split('.')))
        while len(l2) < len(l1): l2.append(0)
        while len(l1) < len(l2): l1.append(0)
        l1.reverse()
        l2.reverse()
        while l1 and l2:
            v1 = l1.pop()
            v2 = l2.pop()
            if v1 > v2: return 1
            if v1 < v2: return -1
        return 0