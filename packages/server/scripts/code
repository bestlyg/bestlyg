def is_prime(num: int) -> bool:
    if num < 2: return False
    v = 2
    while v ** 2 <= num:
        if num % v == 0: return False
        v += 1
    return True
class Solution:
    def countPrimeSetBits(self, left: int, right: int) -> int:
        return sum(
            is_prime(bin(num).count('1'))
            for num in range(left, right + 1)
        )