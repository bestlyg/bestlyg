{
    "name": "@bestlyg/common",
    "version": "0.0.0",
    "description": "",
    "license": "ISC",
    "author": "bestlyg",
    "type": "module",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "exports": {
        "./server": {
            "import": {
                "types": "./dist/server/index.d.mts",
                "default": "./dist/server/index.mjs"
            },
            "require": {
                "types": "./dist/server/index.d.cts",
                "default": "./dist/server/index.cjs"
            }
        },
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./*": {
            "import": "./*",
            "require": "./*",
            "default": "./*"
        }
    },
    "scripts": {
        "build:openapi": "node scripts/openapi.mjs",
        "build:idl": "node scripts/idl.mjs",
        "build": "rimraf dist && pnpm build:openapi && unbuild",
        "dev": "unbuild --watch"
    },
    "dependencies": {
        "axios": "catalog:",
        "chalk": "catalog:",
        "crypto-js": "^4.2.0",
        "dayjs": "catalog:",
        "eventemitter3": "catalog:",
        "figlet": "^1.8.1",
        "fs-extra": "catalog:",
        "immer": "catalog:",
        "lodash-es": "catalog:",
        "metadata-file": "workspace:^",
        "module-checker": "workspace:^",
        "nanoid": "^5.1.5",
        "tapable": "catalog:",
        "zod": "catalog:"
    },
    "devDependencies": {
        "@bestlyg/common": "workspace:^",
        "@bestlyg/server": "workspace:^",
        "@types/crypto-js": "^4.2.2",
        "@types/express": "catalog:",
        "@types/figlet": "^1.7.0",
        "@types/fs-extra": "catalog:",
        "@types/lodash-es": "catalog:",
        "@types/node": "catalog:",
        "@types/react": "catalog:",
        "@xidl/typescript-client": "workspace:^",
        "@xidl/typescript-server": "workspace:^",
        "change-case": "catalog:",
        "express": "^4.21.2",
        "react": "catalog:",
        "react-router": "^7.5.1",
        "rimraf": "catalog:",
        "typedoc": "^0.28.3",
        "typedoc-plugin-markdown": "^4.6.2",
        "typescript": "catalog:",
        "unbuild": "catalog:"
    },
    "peerDependencies": {
        "react": "catalog:",
        "react-router": "catalog:"
    }
}
