{
    "name": "md2resume",
    "version": "0.0.1",
    "author": "bestlyg",
    "files": [
        "packages.json"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "scripts": {
        "build": "rimraf dist && unbuild --config ./node_modules/@bestlyg/common/build.config.ts",
        "dev": "unbuild --config ./node_modules/@bestlyg/common/build.config.ts --watch"
    },
    "dependencies": {
        "@ant-design/icons-svg": "catalog:",
        "lodash": "catalog:",
        "marked": "catalog:",
        "marked-directive": "catalog:",
        "tapable": "catalog:"
    },
    "devDependencies": {
        "@bestlyg/cli": "workspace:^",
        "@bestlyg/common": "workspace:^",
        "@types/lodash": "catalog:",
        "@types/node": "catalog:",
        "rimraf": "catalog:",
        "typescript": "catalog:",
        "unbuild": "catalog:"
    }
}
