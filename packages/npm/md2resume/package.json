    "author": "bestlyg",
    "type": "module",
    "main": "./dist/lib/index.cjs",
    "types": "./dist/types/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/types/index.d.ts"
            ]
        }
