        "marked-directive": "^1.0.5",
        "rc-resize-observer": "^1.4.0",
        "react": "catalog:",
        "react-dom": "catalog:",
        "tapable": "^2.2.1"
    },
    "devDependencies": {
        "@bestlyg/cli": "workspace:^",
        "@bestlyg/config": "workspace:^",
        "@bestlyg/data": "workspace:^",
        "@types/lodash": "catalog:",
        "@types/node": "catalog:",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@vitejs/plugin-react": "catalog:",
