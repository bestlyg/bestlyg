{
    "name": "@xidl/playground",
    "version": "0.0.0",
    "keywords": [
        "idl"
    ],
    "type": "module",
    "main": "./dist/lib/index.cjs",
    "types": "./dist/types/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/esm/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/lib/index.cjs",
            "default": "./dist/esm/index.js"
        },
        "./*": {
            "import": "./*",
            "require": "./*",
            "default": "./*"
        },
        "./package.json": "./package.json"
    },
    "author": "bestlyg@foxmail.com",
    "homepage": "https://github.com/bestlyg/bestlyg/tree/master/packages/npm/xidl",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^22.7.5",
        "typescript": "^5.6.2",
        "@xidl/core": "workspace:^",
        "@xidl/shared": "workspace:^",
        "@xidl/ecma-core": "workspace:^",
        "@xidl/ecma-client": "workspace:^",
        "@xidl/ecma-server": "workspace:^",
        "tsx": "^4.19.1"
    },
    "scripts": {
        "start": "tsx src/index.ts",
        "build": "echo 'playground build'"
    }
}
