{
    "name": "@xidl/shared",
    "version": "0.0.0",
    "keywords": [
        "idl"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "author": "bestlyg@foxmail.com",
    "homepage": "https://github.com/bestlyg/bestlyg/tree/master/packages/npm/xidl",
    "license": "MIT",
    "devDependencies": {
        "@types/fs-extra": "catalog:",
        "@types/lodash-es": "catalog:",
        "@types/node": "catalog:",
        "@types/tapable": "catalog:",
        "@xidl/shared": "workspace:^",
        "rimraf": "catalog:",
        "typescript": "catalog:",
        "unbuild": "catalog:"
    },
    "scripts": {
        "prepublishOnly": "pnpm build",
        "build": "rimraf dist && unbuild --config ./node_modules/@xidl/shared/build.config.ts",
        "dev": "unbuild --config ./node_modules/@xidl/shared/build.config.ts --watch"
    },
    "dependencies": {
        "change-case": "catalog:",
        "fs-extra": "catalog:",
        "lodash-es": "catalog:",
        "protobufjs": "^7.5.0",
        "tapable": "catalog:"
    }
}
