{
    "name": "best-uploader",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/bestlyg/bestlyg.git",
        "directory": "packages/npm/uploader"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "keywords": [
        "uploader"
    ],
    "author": "bestlyg",
    "type": "module",
    "main": "./dist/lib/index.cjs",
    "types": "./dist/types/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/types/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/lib/index.cjs",
            "default": "./dist/esm/index.js"
        },
        "./globals": {
            "types": "./dist/types/globals.d.ts",
            "import": "./dist/esm/globals.js",
            "require": "./dist/lib/globals.cjs",
            "default": "./dist/esm/globals.js"
        },
        "./*": {
            "import": "./*",
            "require": "./*",
            "default": "./*"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "modern build",
        "prepublishOnly": "pnpm build",
        "dev": "modern build --watch"
    },
    "devDependencies": {
        "@modern-js/module-tools": "^2.50.0",
        "@types/node": "^20.14.11",
        "@types/ramda": "^0.30.0",
        "typescript": "^5.5.3"
    },
    "dependencies": {
        "p-queue": "^8.0.1",
        "ramda": "^0.30.0",
        "tapable": "^2.2.1"
    }
}
