{
    "name": "best-uploader",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/bestlyg/bestlyg.git",
        "directory": "packages/npm/uploader"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "keywords": [
        "uploader"
    ],
    "author": "bestlyg",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "scripts": {
        "prepublishOnly": "pnpm build",
        "build": "rimraf dist && unbuild --config ./node_modules/@bestlyg/common/build.config.ts",
        "dev": "unbuild --config ./node_modules/@bestlyg/common/build.config.ts --watch"
    },
    "devDependencies": {
        "@bestlyg/cli": "workspace:^",
        "@bestlyg/common": "workspace:^",
        "@types/node": "catalog:",
        "@types/ramda": "catalog:",
        "rimraf": "catalog:",
        "typescript": "catalog:",
        "unbuild": "catalog:"
    },
    "dependencies": {
        "p-queue": "^8.0.1",
        "ramda": "catalog:",
        "tapable": "catalog:"
    }
}
