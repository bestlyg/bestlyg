        "test": "pnpm build && pnpm exec lessc \"./tests/basic.less\""
    },
    "keywords": [
        "css",
        "less",
        "less.js",
        "plugin",
        "less css",
        "less plugin",
        "less functions",
        "package.json"
    ],
    "license": "MIT",
    "files": [
        "lib",
        "package.json",
        "README.md"
    ],
    "dependencies": {
        "@less-plugins/shared": "workspace:^",
        "lodash.get": "^4.4.2"
    },
    "devDependencies": {
        "@types/less": "^3.0.6",
        "@types/lodash.get": "^4.4.9",
        "less": "^4.2.0",
        "rimraf": "^5.0.5",
        "typescript": "^5.4.5"
    }
}
