{
    "name": "@less-plugins/get-hashed-version",
    "version": "0.0.4",
    "description": "get a hashed version from closest package.json",
    "homepage": "https://github.com/bestlyg/bestlyg/tree/master/packages/npm/less-plugins",
    "author": "BestLyg",
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "scripts": {
        "clean": "rimraf ./lib",
        "build": "tsc -p tsconfig.json",
        "dev": "tsc -p tsconfig.json --watch",
        "prepublishOnly": "pnpm build",
        "test": "pnpm build && pnpm exec lessc \"./tests/basic.less\""
    },
    "keywords": [
        "css",
        "less",
        "less.js",
        "plugin",
        "less css",
        "less plugin",
        "less functions",
        "hash",
        "version"
    ],
    "license": "MIT",
    "files": [
        "lib",
        "package.json",
        "README.md"
    ],
    "peerDependencies": {
        "@emotion/hash": "*"
    },
    "peerDependenciesMeta": {
        "@emotion/hash": {
            "optional": true
        }
    },
    "dependencies": {
        "@less-plugins/shared": "workspace:^",
        "@emotion/hash": "0.9.1",
        "@less-plugins/get-package-json": "workspace:^"
    },
    "devDependencies": {
        "@types/less": "^3.0.6",
        "less": "^4.2.0",
        "rimraf": "^5.0.5",
        "typescript": "^5.4.5"
    }
}
