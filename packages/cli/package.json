{
    "name": "@bestlyg/cli",
    "version": "0.0.0",
    "author": "bestlyg",
    "type": "module",
    "main": "./dist/lib/index.cjs",
    "types": "./dist/esm/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/esm/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/esm/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/lib/index.cjs",
            "default": "./dist/esm/index.js"
        },
        "./globals": {
            "types": "./dist/esm/globals.d.ts",
            "import": "./dist/esm/globals.js",
            "require": "./dist/lib/globals.cjs",
            "default": "./dist/esm/globals.js"
        },
        "./*": {
            "import": "./*",
            "require": "./*",
            "default": "./*"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "run:dev": "cross-env BEST_DEV=1 node --max-old-space-size=4096 bin/best.js",
        "run:prod": "node --max-old-space-size=4096 bin/best.js",
        "build": "modern build",
        "prepublishOnly": "pnpm build",
        "dev": "modern build --watch"
    },
    "bin": {
        "@bestlyg/cli": "./bin/best.js",
        "best": "./bin/best.js"
    },
    "devDependencies": {
        "@bestlyg/cli": "workspace:^",
        "@types/doctrine": "^0.0.9",
        "@types/figlet": "^1.5.8",
        "@types/inquirer": "^9.0.7",
        "@types/lodash": "^4.17.7",
        "@types/minimist": "^1.2.5",
        "@types/node": "^20.14.13",
        "@types/ramda": "^0.30.1",
        "@types/rimraf": "^4.0.5",
        "@modern-js/module-tools": "^2.58.0",
        "@types/semver": "^7.5.8",
        "@types/yargs": "^17.0.32",
        "cross-env": "^7.0.3"
    },
    "keywords": [],
    "license": "ISC",
    "dependencies": {
        "@manypkg/get-packages": "^2.2.1",
        "@types/jsdom": "^21.1.7",
        "adm-zip": "^0.5.14",
        "axios": "^1.6.8",
        "chalk": "^5.3.0",
        "change-case": "^5.4.4",
        "cheerio": "1.0.0-rc.12",
        "commander": "^11.1.0",
        "dayjs": "^1.11.12",
        "doctrine": "^3.0.0",
        "dotenv": "^16.4.5",
        "figlet": "^1.7.0",
        "inquirer": "^10.1.8",
        "jsdom": "^24.1.0",
        "lodash": "^4.17.21",
        "minimist": "^1.2.8",
        "ora": "^8.0.1",
        "p-limit": "^6.1.0",
        "p-queue": "^8.0.1",
        "ramda": "^0.30.0",
        "rimraf": "^5.0.9",
        "semver": "^7.6.0",
        "typescript": "^5.5.4",
        "yargs": "^17.7.2",
        "zod": "^3.23.8",
        "zx": "^8.1.4"
    }
}
