{
    "name": "@bestlyg/cli",
    "version": "0.0.0",
    "author": "bestlyg",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./globals": {
            "import": {
                "types": "./dist/globals.d.mts",
                "default": "./dist/globals.mjs"
            },
            "require": {
                "types": "./dist/globals.d.cts",
                "default": "./dist/globals.cjs"
            }
        },
        "./*": {
            "import": "./*",
            "require": "./*",
            "default": "./*"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "rimraf dist && unbuild --config ./node_modules/@bestlyg/common/build.config.ts",
        "dev": "unbuild --config ./node_modules/@bestlyg/common/build.config.ts --watch"
    },
    "bin": {
        "@bestlyg/cli": "./bin/best.js",
        "best": "./bin/best.js"
    },
    "keywords": [],
    "license": "ISC",
    "dependencies": {
        "@bestlyg/cli": "workspace:^",
        "@bestlyg/common": "workspace:^",
        "@manypkg/get-packages": "^2.2.1",
        "@types/adm-zip": "^0.5.7",
        "@types/doctrine": "catalog:",
        "@types/figlet": "^1.7.0",
        "@types/fs-extra": "catalog:",
        "@types/inquirer": "catalog:",
        "@types/jsdom": "catalog:",
        "@types/lodash": "catalog:",
        "@types/minimist": "catalog:",
        "@types/node": "catalog:",
        "@types/semver": "catalog:",
        "adm-zip": "^0.5.16",
        "axios": "catalog:",
        "cheerio": "1.0.0",
        "commander": "catalog:",
        "cross-env": "catalog:",
        "dayjs": "catalog:",
        "doctrine": "^3.0.0",
        "dotenv": "catalog:",
        "figlet": "^1.7.0",
        "fs-extra": "catalog:",
        "inquirer": "^10.1.8",
        "jsdom": "^24.1.0",
        "lodash": "catalog:",
        "metadata-file": "workspace:^",
        "minimist": "^1.2.8",
        "ora": "^8.0.1",
        "semver": "^7.6.0",
        "typescript": "catalog:",
        "winston": "^3.14.2",
        "zod": "catalog:",
        "zx": "catalog:"
    },
    "devDependencies": {
        "rimraf": "catalog:",
        "unbuild": "catalog:"
    }
}
