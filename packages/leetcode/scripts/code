class Solution:
    def getRow(self, rowIndex: int) -> List[int]:
        dp = [1]
        if rowIndex == 0: return dp
        for i in range(rowIndex):
            next_dp = [1]
            for j in range(1, len(dp)):
                next_dp.append(dp[j] + dp[j - 1])
            next_dp.append(1)
            dp = next_dp
        return dp