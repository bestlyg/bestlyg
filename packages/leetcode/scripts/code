class FoodRatings:
    def __init__(self, foods: List[str], cuisines: List[str], ratings: List[int]):
        self.map = defaultdict(SortedDict)
        self.foods = dict()
        for i in range(len(foods)):
            food = foods[i]
            cuishne = cuisines[i]
            rating = ratings[i]
            self.foods[food] = (cuishne, rating)
            if not rating in self.map[cuishne]: self.map[cuishne][rating] = SortedList()
            self.map[cuishne][rating].add(food)
    def changeRating(self, food: str, newRating: int) -> None:
        cuishne, rating = self.foods[food]
        self.foods[food] = (cuishne, newRating)
        self.map[cuishne][rating].remove(food)
        if not self.map[cuishne][rating]: del self.map[cuishne][rating]
        if not newRating in self.map[cuishne]: self.map[cuishne][newRating] = SortedList()
        self.map[cuishne][newRating].add(food)
    def highestRated(self, cuishne: str) -> str:
        return self.map[cuishne].peekitem()[1][0]