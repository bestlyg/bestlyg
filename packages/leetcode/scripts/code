class Solution:
    def clearStars(self, s: str) -> str:
        res = list(s)
        q = []
        n = len(s)
        for i in range(n):
            if s[i] == '*':
                res[-heappop(q)[1]] = ''
                res[i] = ''
            else:
                heappush(q, (s[i], -i))
        return ''.join(res)