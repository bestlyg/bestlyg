class Solution:
    def kthCharacter(self, k: int) -> str:
        cur = 'a' 
        for _ in range(k):
            ncur = ''
            for c in cur:
                ncur += chr((ord(c) - ord('a') + 1) % 26 + ord('a'))
            cur += ncur
            if len(cur) > k: break
        return cur[k - 1]