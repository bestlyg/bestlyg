class Solution:
    def minMaxDifference(self, num: int) -> int:
        arr = list(map(int, str(num)))
        n = len(arr)
        nmin = arr[0]
        for i in range(n):
            if arr[i] != 9:
                nmin = arr[i]
                break
        vmax = int(''.join(['9' if v == nmin else str(v) for v in arr]))
        vmin = int(''.join(['0' if v == arr[0] else str(v) for v in arr]))
        return vmax - vmin