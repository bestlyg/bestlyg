class Solution:
    def maximumLength(self, nums: List[int], k: int) -> int:
        n = len(nums)
        for i in range(n): nums[i] %= k
        dp = [[0 for _ in range(k)] for _ in range(k)]
        res = 0
        for i in range(n):
            num = nums[i]
            for cur in range(k):
                dp[cur][num] = dp[num][cur] + 1
                res = max(res, dp[cur][num])
        return res