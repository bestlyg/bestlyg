class Solution:
    def numOfUnplacedFruits(self, fruits: List[int], baskets: List[int]) -> int:
        res = 0
        used = set()
        for fruit in fruits:
            f = False
            for i in range(len(baskets)):
                if baskets[i] >= fruit and i not in used:
                    used.add(i)
                    f = True
                    break
            if not f: res += 1
        return res