class Solution:
    def uniquePathsWithObstacles(self, g: List[List[int]]) -> int:
        n = len(g)
        m = len(g[0])
        dp = [[0] * (m + 1) for _ in range(n + 1)]
        dp[1][1] = g[0][0] ^ 1
        for i in range(1, n + 1):
            for j in range(1, m + 1):
                if not g[i - 1][j - 1]:
                    dp[i][j] += dp[i - 1][j] + dp[i][j - 1]
        return dp[n][m]