class Solution:
    def jump(self, nums: List[int]) -> int:
        n = len(nums)
        dp = [inf] * n
        dp[0] = 0
        for i in range(n):
            num = nums[i]
            for j in range(i + 1, min(n, i + num + 1)):
                dp[j] = min(dp[j], dp[i] + 1)
        return dp[n - 1]