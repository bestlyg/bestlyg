class Solution:
    def minDominoRotations(self, tops: List[int], bottoms: List[int]) -> int:
        n = len(tops)
        res = inf
        for num in range(1, 7):
            f = True 
            cnt1 = cnt2 = 0
            for i in range(n):
                if tops[i] != num and bottoms[i] != num:
                    f = False
                    break
                if tops[i] != num:
                    cnt1 += 1
                if bottoms[i] != num:
                    cnt2 += 1
            if f: res = min(res, cnt1, cnt2)
        return res if res != inf else -1