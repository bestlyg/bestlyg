class Solution:
    def possibleStringCount(self, word: str) -> int:
        n = len(word)
        def run(idx: int, f: bool) -> int:
            if idx == n: return 1
            res = run(idx + 1, f)
            if f and idx + 1 < n and word[idx] == word[idx + 1]:
                res += run(idx + 2, False)
            return res
        return run(0, True)