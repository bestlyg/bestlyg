class Solution:
    def getLongestSubsequence(self, words: List[str], groups: List[int]) -> List[str]:
        n = len(words)
        def g(cur: int) -> List[str]:
            res = []
            for i in range(n):
                if groups[i] == cur:
                    res.append(words[i])
                    cur ^= 1
            return res
        a1 = g(0)
        a2 = g(1)
        return a1 if len(a1) > len(a2) else a2