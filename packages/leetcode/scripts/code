class Solution:
    def subsetXORSum(self, nums: List[int]) -> int:
        def dfs(idx: int, arr: List[int]) -> int:
            if idx == len(nums):
                num = 0
                for v in arr:
                    num ^= v
                return num
            res = dfs(idx + 1, arr)
            arr.append(nums[idx])
            res += dfs(idx + 1, arr)
            arr.pop()
            return res
        return dfs(0, [])