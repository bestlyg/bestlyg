class Solution:
    def minimumSubarrayLength(self, nums: List[int], t: int) -> int:
        res = inf
        for i in range(len(nums)):
            for j in range(i + 1):
                v = 0
                for k in range(j, i + 1):
                    v |= nums[k]
                if v >= t:
                    res = min(res, i - j + 1)
        return res if res != inf else -1