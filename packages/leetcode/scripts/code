class Solution:
    def zeroFilledSubarray(self, nums: List[int]) -> int:
        res = 0
        n = len(nums)
        i = 0
        while i < n:
            if nums[i] == 0:
                cnt = 1
                res += cnt
                while i + 1 < n and nums[i + 1] == 0:
                    i += 1
                    cnt += 1
                    res += cnt
            i += 1
        return res