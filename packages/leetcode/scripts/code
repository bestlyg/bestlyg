class Solution:
    def minimumArea(self, grid: List[List[int]]) -> int:
        t = l = inf
        b = r = -inf
        for i in range(len(grid)):
            for j in range(len(grid[i])):
                if grid[i][j] == 1:
                    t = min(t, i)
                    l = min(l, j)
                    b = max(b, i)
                    r = max(r, j)
        return (b - t + 1) * (r - l + 1)