class Solution:
    def countHillValley(self, nums: List[int]) -> int:
        res = 0
        arr = []
        for num in nums:
            if not arr or arr[-1] != num:
                arr.append(num)
        return sum(arr[i - 1] < arr[i] > arr[i + 1] or arr[i - 1] > arr[i] < arr[i + 1] for i in range(1, len(arr) - 1))