class Solution:
    def countInterestingSubarrays(self, nums: List[int], modulo: int, k: int) -> int:
        sums = [0]
        for num in nums: sums.append(int(num % modulo == k) + sums[-1])
        counter = Counter()
        res = 0
        for num in sums: 
            if num >= k:
                res += counter[(num - k) % modulo]
            counter[num % modulo] += 1
        return res