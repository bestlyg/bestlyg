class Solution:
    def countMaxOrSubsets(self, nums: List[int]) -> int:
        n = len(nums)
        res = 0
        nmax = 0
        for num in nums: nmax |= num
        for mask in range(2 ** n):
            num = 0
            for i in range(n):
                if mask & (1 << i):
                    num |= nums[i]
            if num == nmax: res += 1
        return res