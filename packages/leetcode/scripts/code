class Solution:
    def matchPlayersAndTrainers(self, players: List[int], trainers: List[int]) -> int:
        players.sort()
        trainers.sort(reverse = True)
        res = 0
        for i in range(len(players)):
            while trainers and trainers[-1] < players[i]:
                trainers.pop()
            if trainers:
                res += 1
                trainers.pop()
        return res