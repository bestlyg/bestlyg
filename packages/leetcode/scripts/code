class Solution:
    def countGood(self, nums: List[int], k: int) -> int:
        counter = Counter()
        l = 0
        v = 0
        res = 0
        for r in range(len(nums)):
            v += counter[nums[r]]
            counter[nums[r]] += 1
            while v >= k:
                counter[nums[l]] -= 1
                v -= counter[nums[l]]
                l += 1
            res += l
        return res