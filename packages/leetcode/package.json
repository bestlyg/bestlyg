{
    "name": "@bestlyg/leetcode",
    "version": "1.0.0",
    "author": "bestlyg",
    "type": "module",
    "main": "./dist/lib/index.cjs",
    "types": "./dist/types/index.d.ts",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/types/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/lib/index.cjs",
            "default": "./dist/esm/index.js"
        },
        "./*": {
            "import": "./*",
            "require": "./*",
            "default": "./*"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "auto": "pnpm create:problem && pnpm -w ca",
        "create:problem": "node scripts/create-problem.mjs",
        "build": "modern build",
        "dev": "modern build --watch"
    },
    "keywords": [],
    "license": "ISC",
    "devDependencies": {
        "@bestlyg/cli": "workspace:^",
        "@bestlyg/config": "workspace:^",
        "@bestlyg/data": "workspace:^",
        "@bestlyg/leetcode": "workspace:^",
        "@modern-js/module-tools": "catalog:",
        "@types/node": "catalog:",
        "axios": "catalog:",
        "chalk": "catalog:",
        "dayjs": "catalog:",
        "fs-extra": "catalog:",
        "leetcode-query": "^1.2.3",
        "typescript": "catalog:"
    }
}
