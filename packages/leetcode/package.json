{
    "name": "@bestlyg/leetcode",
    "version": "1.0.0",
    "type": "commonjs",
    "scripts": {
        "build": "ts-node -r tsconfig-paths/register scripts/build.ts",
        "batch": "ts-node -r tsconfig-paths/register scripts/batchLeetCode.ts",
        "readme": "ts-node -r tsconfig-paths/register scripts/createLeetCodeReadme.ts",
        "markdown": "ts-node -r tsconfig-paths/register scripts/createLeetCodeMarkDown.ts",
        "check": "ts-node -r tsconfig-paths/register scripts/check.ts",
        "auto": "pnpm markdown && pnpm readme && pnpm -w ca",
        "js": "node scripts/script.js",
        "ts": "ts-node -r tsconfig-paths/register scripts/script.ts",
        "create:readme": "node scripts/create-readme.mjs",
        "update:problem": "node scripts/update-problem.mjs"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@bestlyg/cli": "workspace:^",
        "@types/node": "^20.14.13",
        "axios": "^1.6.8",
        "chalk": "~4",
        "dayjs": "^1.11.12",
        "fs-extra": "^11.1.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "4.0.0",
        "tslib": "^2.6.2",
        "typescript": "^5.5.4"
    }
}
