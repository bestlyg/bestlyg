    "check": "ts-node -r tsconfig-paths/register scripts/check.ts",
    "auto": "pnpm markdown && pnpm readme",
    "js": "node scripts/script.js",
    "ts": "ts-node -r tsconfig-paths/register scripts/script.ts"
