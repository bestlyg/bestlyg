class Solution:
    def clearDigits(self, s: str) -> str:
        pre_idx = -1
        for i in range(len(s)):
            if ord('0') <= ord(s[i]) <= ord('9'):
                return self.clearDigits(s[0: pre_idx] + s[pre_idx + 1:i ] + s[i + 1:])
            elif ord('a') <= ord(s[i]) <= ord('z'):
                pre_idx = i
        return s