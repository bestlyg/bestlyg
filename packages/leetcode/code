class Solution:
    def sumDigitDifferences(self, nums: List[int]) -> int:
        res = 0
        maps = [defaultdict(int) for _ in range(len(str(nums[0])))]
        for i, num in enumerate(nums):
            for j, val in enumerate(list(str(num))):
                res += i - maps[j][val]
                maps[j][val] += 1
        return res