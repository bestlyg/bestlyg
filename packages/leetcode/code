class Solution:
    def canMakeSquare(self, grid: List[List[str]]) -> bool:
        for i in range(2):
            for j in range(2):
                counter = Counter()
                counter[grid[i][j]] += 1
                counter[grid[i + 1][j]] += 1
                counter[grid[i][j + 1]] += 1
                counter[grid[i + 1][j + 1]] += 1
                if any(cnt >= 3 for cnt in counter.values()):
                    return True
        return False