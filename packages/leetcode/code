class Solution:
    def minAnagramLength(self, s: str) -> int:
        n = len(s)
        def cmp(c: Counter, cnt: int) -> bool:
            for start in range(i + 1, n, i + 1):
                if Counter(s[start: start + i + 1]) != c:
                    return False
            return True
        for i, c in enumerate(s):
            if n % (i + 1) == 0 and cmp(Counter(s[0: i + 1]), i + 1):
                return i + 1
        return len(s)