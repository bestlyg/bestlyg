class Solution:
    def findJudge(self, n: int, trust: List[List[int]]) -> int:
        carr = [[] for _ in range(n + 1)]
        parr = [[] for _ in range(n + 1)]
        for p1, p2 in trust:
            parr[p2].append(p1)
            carr[p1].append(p2)
        for i in range(1, n + 1):
            if not carr[i] and len(parr[i]) == n - 1:
                return i
        return -1