class Solution:
    def maxStrength(self, nums: List[int]) -> int:
        res = 1
        n = len(nums)
        nums.sort()
        while nums and nums[-1] > 0: res *= nums.pop()
        while len(nums) >= 2 and nums[0] * nums[1] > 0: res *= nums.pop(0) * nums.pop(0)
        if n == len(nums): return max(nums)
        return res