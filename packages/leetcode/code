class Solution:
    def findWinningPlayer(self, skills: List[int], k: int) -> int:
        nmax = max(skills)
        cnt = cur = 0
        for i in range(1, len(skills)):
            if skills[cur] < skills[i]:
                cur = i
                cnt = 0
            cnt += 1
            if cnt == k or skills[cur] == nmax: return cur