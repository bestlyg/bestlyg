class Solution:
    def waysToReachStair(self, k: int) -> int:
        @cache
        def dfs(cur: int, jump: int, down: bool) -> int:
            if cur > k + 1: return 0
            res = 0
            if cur == k: res += 1
            res += dfs(cur + 2 ** jump, jump + 1, False)
            if cur != 0 and not down: res += dfs(cur - 1, jump, True)
            return res
        return dfs(1, 0, False)