class Solution:
    def countQuadruplets(self, nums: List[int]) -> int:
        n = len(nums)
        arr = [0] * n
        res = 0
        for l in range(n):
            for j in range(l - 1):
                if nums[j] < nums[l]:
                    res += arr[j]
            cnt = 0
            for j in range(l):
                if nums[j] > nums[l]:
                    arr[j] += cnt
                if nums[j] < nums[l]:
                    cnt += 1
        return res