{
    "files.associations": {
        ".babelrc": "json",
        "*.js": "javascriptreact",
        "*.ts": "typescriptreact",
        "iostream": "cpp",
        "vector": "cpp",
        "unordered_set": "cpp",
        "string": "cpp",
        "iosfwd": "cpp",
        "ostream": "cpp",
        "list": "cpp",
        "queue": "cpp",
        "random": "cpp",
        "algorithm": "cpp",
        "array": "cpp",
        "string_view": "cpp",
        "iterator": "cpp",
        "__config": "cpp",
        "__bit_reference": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "__tree": "cpp",
        "deque": "cpp",
        "filesystem": "cpp",
        "initializer_list": "cpp",
        "ios": "cpp",
        "map": "cpp",
        "regex": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
        "__threading_support": "cpp",
        "thread": "cpp",
        "bitset": "cpp",
        "__mutex_base": "cpp",
        "mutex": "cpp",
        "shared_mutex": "cpp",
        "numeric": "cpp",
        "__nullptr": "cpp",
        "types.h": "c",
        "temp": "cpp",
        "climits": "cpp",
        "ciso646": "cpp",
        "stack": "cpp",
        "sstream": "cpp",
        "valarray": "cpp",
        "utility": "cpp",
        "chrono": "cpp",
        "limits": "cpp",
        "ratio": "cpp",
        "istream": "cpp"
    },
    "C_Cpp.errorSquiggles": "disabled",

    "files.watcherExclude": {
        "**/.nx/**": true,
        "**/data/**": true,
        "**/dist/**": true,
        "**/encrypt-src/**": true,
        "**/static/**": true,
        "**/target/**": true
    },
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.iconTheme": "vs-seti",
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.confirmSync": false,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "[jsonc]": {
        "editor.defaultFormatter": "oxc.oxc-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.openRepositoryInParentFolders": "always",
    "explorer.confirmDelete": false,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[mdx]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[less]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.eol": "\n",
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "[javascriptreact]": {
        "editor.defaultFormatter": "oxc.oxc-vscode"
    },
    "cSpell.userWords": ["bestlyg", "bundleless", "leetcode", "xidl"],
    "prettier.prettierPath": "./node_modules/prettier",
    "editor.wordWrap": "on"
}
