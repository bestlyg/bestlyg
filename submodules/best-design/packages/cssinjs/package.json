{
    "extension": {
        "extends": [
            "../../package.base.json"
        ],
        "vars": {
            "name": "cssinjs",
            "type": "packages"
        },
        "meta": {
            "version": "0.0.1"
        }
    },
    "name": "@best-design/cssinjs",
    "version": "0.0.6",
    "description": "A best react ui components for cssinjs.",
    "main": "lib/index.js",
    "scripts": {
        "extend-package-json": "extend-package-json",
        "clean": "best-tools clean",
        "build": "father build",
        "update-package-json": "best-tools update-package-json",
        "update-version": "best-tools update-version",
        "build:umd": "best-tools build --type umd",
        "build:cjs": "best-tools build --type cjs",
        "build:esm": "best-tools build --type esm",
        "storybook": "BEST_DESIGN_DEMO_COMPONENT=packages/cssinjs pnpm -w storybook"
    },
    "keywords": [
        "best",
        "bestlyg",
        "component",
        "components",
        "design",
        "framework",
        "frontend",
        "react",
        "react-component",
        "ui",
        "cssinjs"
    ],
    "author": "bestlyg@foxmail.com",
    "license": "MIT",
    "dependencies": {
        "@emotion/hash": "^0.9.1",
        "@emotion/unitless": "^0.8.1",
        "csstype": "^3.1.3"
    },
    "devDependencies": {
        "@best-design/tools": "workspace:^",
        "@types/react": "^18.2.22",
        "@types/react-dom": "^18.2.7",
        "eslint-plugin-prettier": "^4.2.1",
        "father": "^4.3.7",
        "less-plugin-get-package": "^0.0.1",
        "package-json-extension": "^0.0.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "typescript": "^5.0.4"
    },
    "contributors": [
        "bestlyg@foxmail.com"
    ],
    "module": "es/index.js",
    "typings": "es/index.d.ts",
    "unpkg": "dist/cssinjs.min.js",
    "homepage": "https://bestlyg.com",
    "bugs": {
        "url": "https://github.com/bestlyg/best-design/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bestlyg/best-design"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "files": [
        "dist",
        "es",
        "lib",
        "src",
        "package.json",
        "README.md"
    ],
    "peerDependencies": {
        "react": ">=16.9.0",
        "react-dom": ">=16.9.0"
    },
    "sideEffects": [
        "**/*.css",
        "**/*.less",
        "**/*.sass",
        "**/*.scss"
    ]
}
